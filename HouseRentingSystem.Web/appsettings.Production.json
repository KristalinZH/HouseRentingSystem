{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "Identity": {
        "SignIn": {
            "RequireConfirmedAccount": true
        },
        "Password": {
            "RequireLowercase": true,
            "RequireUppercase": true,
            "RequireNonAlphanumeric": true,
            "RequiredLength": 8
        }
    }
}
